<?xml version="1.0" encoding="UTF-8"?>
<Package>
    <DisplayName>OpenTodoList</DisplayName>
    <Description>The OpenTodoList application.</Description>
    <Version>@OPENTODOLIST_VERSION@</Version>
    <ReleaseDate>@INSTALLER_DATE@</ReleaseDate>
    <Name>net.rpdev.OpenTodoList</Name>
    <!-- This causes issues in the Windows installer on update:
    <Licenses>
        <License name="GNU GPL v3" file="COPYING" />
    </Licenses>-->
    <Default>true</Default>
    <Script>script.js</Script>
    <ForcedInstallation>true</ForcedInstallation>
    <RequiresAdminRights>true</RequiresAdminRights>
</Package>
